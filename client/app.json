{
  "expo": {
    "name": "ApiRoot",
    "slug": "ApiRoot",
    "description": "Gestor de llamadas API REST",
    "privacy": "unlisted",
    "sdkVersion": "35.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "es.larryrider.apiroot"
    },
    "android": {
      "package": "es.larryrider.apiroot",
      "versionCode": 1,
      "adaptiveIcon": {
        "backgroundColor": "#FFFFFF",
        "foregroundImage": "./assets/icon_foreground.png"
      }
    },
    "notification": {
      "icon": "./assets/icon.png",
      "color": "#FFF",
      "androidMode": "default",
      "androidCollapsedTitle": "ApiRoot"
    }
  }
}